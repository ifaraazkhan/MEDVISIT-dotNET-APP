					<div class="row align-items-center">
						<div class="col-md-12 col-12">
							
							<h2 class="breadcrumb-title">Dashboard</h2>
						</div>
					</div>
				</div>
			</div>
			<!-- /Breadcrumb -->
            </asp:ContentPlaceHolder>

            <!-- Page Content -->
			<div class="content">
				<div class="container-fluid">

					<div class="row">
