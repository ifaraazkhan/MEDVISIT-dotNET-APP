							<div class="profile-sidebar">
                                <asp:ContentPlaceHolder ID="ContentPlaceHolder_dr_profile" runat="server">
								<div class="widget-profile pro-widget-content">
									<div class="profile-info-widget">
										<a href="#" class="booking-doc-img" style="padding:0px">
											<img src="assets/img/doctors/doctor_icon.png" alt="User Image">
										</a>
										<div class="profile-det-info">
											<h3>Dr. <asp:Label ID="lbl_doctor_name" runat="server" Text=""></asp:Label></h3>
											
											<div class="patient-details">
												<h5 class="mb-0">
                                                    <asp:Label ID="lbl_dr_degree" runat="server" Text=""></asp:Label></h5>
											</div>
										</div>
									</div>
								</div>
                                </asp:ContentPlaceHolder>

								<div class="dashboard-widget">
									<nav class="dashboard-menu">
										<ul id="navigation_menu">
											<li>
												<a href="Dashboard.aspx">
													<i class="fas fa-columns"></i>
													<span>Dashboard</span>
												</a>
											</li>
											<li>
												<a href="appointments.aspx">
													<i class="fas fa-calendar-check"></i>
													<span>Today's Appointments</span>
												</a>
											</li>
											
											<li>
												<a href="calendar.aspx">
													<i class="fas fa-calendar"></i>
													<span>Manage Calendar</span>
												</a>
											</li>
											
											<li>
												<a href="Profile.aspx">
													<i class="fas fa-columns"></i>
													<span>Profile</span>
												</a>
											</li>
										
											<li>
												<a href="Settings.aspx">
													<i class="fas fa-user-cog"></i>
													<span>Settings</span>
												</a>
											</li>
											
										</ul>
                                   
									</nav>
								</div>
                                
							</div>
							<!-- /Profile Sidebar -->
							
						</div>
                        </asp:ContentPlaceHolder>

                        <asp:ContentPlaceHolder ID="ContentPlaceHolder_mainArea" runat="server">

                        </asp:ContentPlaceHolder>

                    </div>

				</div>
                <footer class="footer">
				
				
				
				<!-- Footer Bottom -->
                <div class="footer-bottom">
					<div class="container-fluid">
					
