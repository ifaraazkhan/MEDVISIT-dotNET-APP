     <!-- Change Doctor Modal -->
		 <div class="modal fade custom-modal" id="change_Profile_modal">
			<div class="modal-dialog modal-dialog-centered">
				<div class="modal-content">
					<div class="modal-header">
						<h5 class="modal-title">Change Doctor Profile</h5>
						<button type="button" class="close" data-dismiss="modal" aria-label="Close">
							<span aria-hidden="true">&times;</span>
						</button>
					</div>
					<div class="modal-body">
                             <asp:ListView ID="ListView_DoctorMapping" runat="server" ItemPlaceholderID="itemPlaceHolder1" OnItemUpdating="ListView_DoctorMapping_ItemUpdating">

                               <LayoutTemplate>
                                   	<div class="appointments">
                                     <asp:PlaceHolder runat="server" ID="itemPlaceHolder1"></asp:PlaceHolder>
                                    </div>
                               </LayoutTemplate>

					           <ItemTemplate>

                                   <!-- Appointment List -->
								<div class="appointment-list" style="padding:10px;margin:10px">
									<div class="profile-info-widget">
										<a href="patient-profile.html" class="booking-doc-img">
											<img src="assets/img/doctors/doctor_icon.png" alt="User Image">
										</a>
										<div class="profile-det-info">
											<h3><a href="#">  <%# Eval("name")%> </a></h3>
											<div class="patient-details">
												
												<h5><i class="fas fa-envelope"></i>   <%# Eval("email")%> </h5>
												<h5 class="mb-0"><i class="fas fa-phone"></i>   <%# Eval("phone")%> </h5>
                                                <p>
                                                    <br />
                                                   
                                                     
                                                    <asp:LinkButton ID="UpdateButton" class="btn btn-sm bg-success-light" ToolTip='make this active' OnClick="UpdateButton_Click" runat="server" CommandArgument='<%#Eval("doctor_id")%>' CommandName="Update">
                                                         <i class="fas fa-check"></i>  SWITCH PROFILE
                                                    </asp:LinkButton>
                                                   
                                                   <asp:Label ID="lbl_id" runat="server" Visible="false" Text='<%# Eval("doctor_id")%>'></asp:Label>
                                                </p>
                                                 	
											</div>
										</div>
									</div>
									
								</div>
								<!-- /Appointment List -->
                               </ItemTemplate>
			
                            </asp:ListView>
					</div>
				</div>
			</div>
		</div>
